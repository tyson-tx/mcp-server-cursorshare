{
  "name": "cursorshare",
  "displayName": "CursorShare",
  "description": "Share and manage your Cursor AI chat logs",
  "version": "0.9.2",
  "engines": {
    "vscode": "^1.95.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished",
    "*"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "cursorshare.showWorkspaces",
        "title": "CursorShare: Show Workspaces"
      },
      {
        "command": "cursorshare.quickShare",
        "title": "CursorShare: Quick Share Current Chat"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "command": "cursorshare.quickShare",
          "group": "navigation",
          "when": "resourceScheme == cursorChat"
        }
      ]
    },
    "configuration": {
      "title": "CursorShare",
      "properties": {
        "cursorshare.apiEndpoint": {
          "type": "string",
          "default": "https://www.cursorshare.com",
          "description": "API endpoint for sharing"
        }
      }
    }
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "pnpm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/axios": "^0.14.4",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@types/vscode": "1.95.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "eslint": "^9.21.0",
    "globals": "^16.0.0"
  },
  "dependencies": {
    "axios": "^1.8.2",
    "dotenv": "^16.4.7",
    "sqlite": "^5.0.1",
    "sqlite3": "^5.1.6"
  },
  "publisher": "cursorShare",
  "repository": {
    "type": "git",
    "url": "https://github.com/tyson-tx/cursorshare.git"
  },
  "icon": "resources/icon.png",
  "galleryBanner": {
    "color": "#C80000",
    "theme": "dark"
  },
  "keywords": [
    "cursor",
    "share",
    "chat",
    "ai"
  ]
}